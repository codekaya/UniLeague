<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>Genel Sıralama | Unileague</title>
    <link rel="stylesheet" href="/bootstrap/dist/css/bootstrap.min.css">
    <script src="/bootstrap/dist/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Inter:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800&amp;display=swap">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script src="https://cdn.datatables.net/1.13.4/js/jquery.dataTables.js"></script>
    
     <script src="js/ranking.js"></script>

</head>
<body class="bg-white">
    <nav class="navbar navbar-dark navbar-expand-md fixed-top navbar-shrink py-3 bg-dark" id="mainNav">
        <div class="container">
          <a class="navbar-brand d-flex align-items-center" href="/">
                    </svg></span><span>UNILEAGUE</span></a><button data-bs-toggle="collapse" class="navbar-toggler" data-bs-target="#navcol-1"><span class="visually-hidden">Toggle navigation</span><span class="navbar-toggler-icon"></span></button>
            <div class="collapse navbar-collapse" id="navcol-1">
                <ul class="navbar-nav mx-auto">
                    <li class="nav-item"><a class="nav-link active" href="/">Anasayfa</a></li>
                    <li class="nav-item"><a class="nav-link" href="#">Karşılaştır</a></li>
                    <li class="nav-item"><a class="nav-link" href="#">Genel Sıralama</a></li>
                    <li class="nav-item"><a class="nav-link" href="#">Hakkımızda</a></li>
                </ul>
                <% if (userType==="loggedIn") { %>
                  <a class="btn btn-primary shadow" role="button" href="user/profile"> &nbsp;Profilim&nbsp;&nbsp; </a>
                  &nbsp;&nbsp;
                  <a class="btn btn-primary shadow" role="button" onclick="logout()">Çıkış Yap</a>
                <% } else { %>
                  <a class="btn btn-primary shadow" role="button" href="/login">Giriş Yap</a>
                <% } %>
                &nbsp;&nbsp;
                </ul><% if (userType==="visitor") { %>
                  <a id="registerBtn" class="btn btn-primary shadow" role="button" href="/register">Kayıt Ol</a>
                <% } %>
            </div>
        </div>
    </nav>
    <br><br><br><br>
        <header class="text-center text-black bg masthead">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="data_table">
                            <div id="tableSearch"></div>
                            <table id="example" class="table table-striped table-bordered" style>
                                <thead class="table">
                                    
                                    <tr>
                                        <th>ÜNİVERSİTE</th>
                                        <th>BÖLGE</th>
                                        <th>İL</th>
                                        <th>TÜR</th>
                                        <th>EĞİTİM PUANI</th>
                                        <th>YURT IMKAN PUANI</th>
                                        <th>ULASIM PUANI</th>
                                        <th>KAMPÜS PUANI</th>
                                        <th>UNILEAGUE PUANI</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <h2>ÜNİVERSITE PUAN TABLOSU </h2>
                                    <% all_uni_info.forEach(uni => { %>
                                        <tr>
                                            
                                            <td>
                                                <a class="link-offset-2 link-offset-3-hover link-underline link-underline-opacity-0 link-underline-opacity-75-hover" style="text-decoration:none" href="/university?id=<%= uni._id %>">
                                                  <%= uni.title %>
                                                </a>
                                              </td>
                                            <td><%= uni.Region  %></td>
                                            <td><%= uni.Province  %></td>
                                            <td><%= uni.Type  %></td>
                                            <td><%= uni.edu_point  %></td>
                                            <td><%= uni.dorm_point  %></td>
                                            <td><%= uni.trans_point  %></td>
                                            <td><%= uni.campus_point  %></td>
                                            <td><%= uni.unileague_point %></td>
                                            
      
                                        </tr>
                                <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        
</body>

</html>
